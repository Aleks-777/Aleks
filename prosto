F = (gravitational_constant * obj.m * body.m)/(r**2)
        body.Fx += F * (body.x - obj.x)/r  # FIXME: нужно вывести формулу...
        body.Fy += F * (body.y - obj.y)/r  # FIXME: нужно вывести формулу...
         ax = body.Fx/body.m
    body.x += body.Vx  # FIXME: не понимаю как менять...
    body.Vx += ax*dt
    ay = body.Fy/body.m
    body.y += body.Vy  # FIXME: не понимаю как менять...
    body.Vy += ay*dt
